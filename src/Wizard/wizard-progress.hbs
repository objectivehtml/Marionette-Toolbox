<div class="wizard-progress-bar">
{{#steps}}
    <a class="wizard-step {{#if options.complete}}{{../completeClassName}}{{else}}{{../disabledClassName}}{{/if}}" data-step="{{options.step}}" {{#if options.title}}title="{{options.title}}"{{/if}}>
        {{#if options.label}}
            <span class="wizard-step-label">{{options.label}}</span>
        {{else}}
            {{#if options.title}}
                <span class="wizard-step-label">{{options.title}}</span>
            {{/if}}
        {{/if}}
    </a>
{{/steps}}
</div>
